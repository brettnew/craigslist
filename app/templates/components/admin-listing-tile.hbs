<ul class="list-group">
  {{#each sortedListings as |listing|}}
    <li class="list-group-item">{{#link-to 'listing' listing.id}} {{listing.title}} {{/link-to}}
      <button class="btn btn-danger btn-sm"{{action 'delete' listing}}>Delete</button>
      {{edit-listing class="editButton" listing=listing categories=categories edit="edit"}}
    </li>
  {{/each}}
</ul>
